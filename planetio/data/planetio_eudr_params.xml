<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
  <data>
    <!-- EUDR endpoint (ACCEPTANCE) -->
    <record id="planetio_param_eudr_endpoint" model="ir.config_parameter">
      <field name="key">planetio.eudr_endpoint</field>
      <field name="value">https://acceptance.eudr.webcloud.ec.europa.eu/tracesnt/ws/EUDRSubmissionServiceV1</field>
    </record>

    <!-- EUDR username (placeholder) -->
    <record id="planetio_param_eudr_user" model="ir.config_parameter">
      <field name="key">planetio.eudr_user</field>
      <field name="value">n00igg11</field>
    </record>

    <!-- EUDR API key (leave empty or placeholder; override in Production) -->
    <record id="planetio_param_eudr_apikey" model="ir.config_parameter">
      <field name="key">planetio.eudr_apikey</field>
      <field name="value">1k7yq0uwofIoxs688vHrDN2nqOSZhiJGHJ5iGK6u</field>
    </record>

    <!-- WS-Security mode: digest (default) | text -->
    <record id="planetio_param_eudr_wsse_mode" model="ir.config_parameter">
      <field name="key">planetio.eudr_wsse_mode</field>
      <field name="value">digest</field>
    </record>

    <!-- Optional: WebServiceClientId (default eudr-test). If you set this, update adapter to read it. -->
    <record id="planetio_param_eudr_wsclient" model="ir.config_parameter">
      <field name="key">planetio.eudr_webservice_client_id</field>
      <field name="value">eudr-test</field>
    </record>
  </data>
</odoo>
