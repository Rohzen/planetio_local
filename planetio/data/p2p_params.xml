<odoo>
  <!-- Endpoint & Key & Opzioni, modificabili da Impostazioni di Sistema -->
  <record id="param_tracer_base_url" model="ir.config_parameter">
    <field name="key">planetio.tracer_base_url</field>
    <field name="value">https://tracer-development.azure.startplanting.org</field>
  </record>

  <record id="param_tracer_endpoint" model="ir.config_parameter">
    <field name="key">planetio.tracer_endpoint</field>
    <field name="value">/api/farm-data</field>
  </record>

  <record id="param_tracer_api_key" model="ir.config_parameter">
    <field name="key">planetio.tracer_api_key</field>
    <field name="value">HxDjBvLErdxn8uBzB5bbEZcw3TswadGe</field>
  </record>

  <record id="param_tracer_buffer_meters" model="ir.config_parameter">
    <field name="key">planetio.tracer_buffer_meters</field>
    <field name="value">30</field>
  </record>

  <record id="param_tracer_commodity" model="ir.config_parameter">
    <field name="key">planetio.tracer_commodity</field>
    <field name="value">coffee</field>
  </record>
</odoo>