<odoo>
  <data noupdate="1">
    <!-- Auth mode: 'token' (default) or 'oauth' -->
    <record id="param_gfw_auth_mode" model="ir.config_parameter">
      <field name="key">planetio.gfw_auth_mode</field>
      <field name="value">token</field>
    </record>

    <!-- OAuth client-credentials settings -->
    <record id="param_gfw_oauth_token_url" model="ir.config_parameter">
      <field name="key">planetio.gfw_oauth_token_url</field>
      <field name="value"></field>
    </record>
    <record id="param_gfw_oauth_client_id" model="ir.config_parameter">
      <field name="key">planetio.gfw_oauth_client_id</field>
      <field name="value"></field>
    </record>
    <record id="param_gfw_oauth_client_secret" model="ir.config_parameter">
      <field name="key">planetio.gfw_oauth_client_secret</field>
      <field name="value"></field>
    </record>
    <record id="param_gfw_oauth_scope" model="ir.config_parameter">
      <field name="key">planetio.gfw_oauth_scope</field>
      <field name="value"></field>
    </record>

    <record id="param_gfw_api_key" model="ir.config_parameter">
      <field name="key">planetio.gfw_api_key</field>
      <field name="value"></field>
    </record>
  
    <record id="param_gfw_api_origin" model="ir.config_parameter">
      <field name="key">planetio.gfw_api_origin</field>
      <field name="value">http://localhost</field>
    </record>
  
    <record id="param_gfw_days_back" model="ir.config_parameter">
      <field name="key">planetio.gfw_days_back</field>
      <field name="value">365</field>
    </record>
  
    <record id="param_gfw_force_bbox" model="ir.config_parameter">
      <field name="key">planetio.gfw_force_bbox</field>
      <field name="value">false</field>
    </record>
  </data>
</odoo>
