<odoo>
  <record id="view_eudr_declaration_form_planetio_lots" model="ir.ui.view">
    <field name="name">view.eudr.declaration.form.planetio_lots</field>
    <field name="model">eudr.declaration</field>
    <field name="inherit_id" ref="planetio.view_eudr_declaration_form"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='net_mass_kg']" position="after">
        <field name="stock_lot_id"/>
      </xpath>
    </field>
  </record>
</odoo>
